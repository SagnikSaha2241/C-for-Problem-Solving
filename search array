#include<stdio.h>
int main()
{
	int arr[1000] ,n ,i,temp;
	printf("How many numbers you want to enter the array: ");
	scanf("%d", &n);
	for(i = 0 ; i < n ; i++)
	{
		scanf("%d", &arr[i]);
	}
	for(i = 0 ; i < n ; i++)
	{
		printf(" %d ",arr[i]);
	}
	printf("Enter The Value That you want to Search: ");
	scanf("%d", &temp);
	
	for(i = 0 ; i < n ; i++)
	{
		if(temp==arr[i])
		{
			printf("%d is found.",temp);
		}
	}
	return 0;
}
	
