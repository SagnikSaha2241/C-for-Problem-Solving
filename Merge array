#include<stdio.h>
int main()
{
	int arr1[1000],arr2[1000] ,arres[1000];
	int n,m,i,s,j,k;
	
	printf("Enter The Number of values in array1: ");
	scanf("%d" ,&n);
	
	printf("Enter The Number of values in array2: ");
	scanf("%d" ,&m);
	
	printf("Enter The Values in 1st array: \n");
	
	for(i = 0 ; i < n ; i++)
	{
		scanf("%d" ,&arr1[i]);
	}
	
	printf("Enter The Values in 2nd array: \n");
	
	for(i = 0 ; i < n ; i++)
	{
		scanf("%d" ,&arr2[i]);
	}
	
	i, j , k = 0 , 0 , 0;
	
	for()
	{
		if(arr1[i] <= arr2[j])
		{
			arres[k] = arr1[i];
			i++;
		}
		else
		{
			arres[k] = arr2[j];
			j++;
		}
		
		k++;
	}
	 
	printf("%d",arres[4]);
	
	
	
	return 0;
	
}
